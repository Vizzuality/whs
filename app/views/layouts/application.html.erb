<!DOCTYPE html>
<html>
  <%= render :partial => "/shared/head" %>
  <body id="<%= feature_type %>">
    <%= render :partial => "/shared/ie6check" if request.env['HTTP_USER_AGENT'] =~ /MSIE/ -%>
    <div id='page_container'>
      <div id='page'>
        <div id='header'>
          <%= render :partial => "/shared/header" %>
        </div>
        <div id='body'>
          <div id="container">
            <%= yield %>
          </div>
        </div>
        <div id='footer'>
          <%= render :partial => "/shared/footer" %>
        </div>
      </div>
    </div>
  </body>
</html>
